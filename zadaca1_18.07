    "def preceden(c):\n",
    "    \n",
    "    if (c=='+') or (c=='-'):\n",
    "        return 1\n",
    "    elif c=='*' or c=='/':\n",
    "         return 2\n",
    "    elif c=='^':\n",
    "        return 3\n",
    "    else:\n",
    "        return -1"
